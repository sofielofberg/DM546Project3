#######################################
# Print the first n Fibonacci numbers
#######################################

# Input
variable n has_type Number is 10;

#######################################
# Computation
#######################################

variable a has_type Number is 0; 
variable b has_type Number is 1; 

variable i has_type Number is 1;
loop_while (i LESS_EQUAL n) {
	write_to_console cast_to String a;
	variable next has_type Number is add(a  b);
	a is b;
	b is next;
	i is add(i 1);
} 
